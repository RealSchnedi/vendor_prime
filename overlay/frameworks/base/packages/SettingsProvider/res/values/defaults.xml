<resources>

    <!-- Display -->
    <integer name="def_screen_off_timeout">120000</integer>
    <!-- Default screen brightness, from 0 to 255.  82 is 32%. -->
    <integer name="def_screen_brightness">82</integer>
    <bool name="def_screen_brightness_automatic_mode">true</bool>

    <!-- Connectivity -->
    <bool name="def_bluetooth_on">false</bool>
    <bool name="def_wifi_on">false</bool>
    <bool name="def_networks_available_notification_on">false</bool>

    <!-- Backup -->
    <string name="def_backup_transport" translatable="false">com.google.android.gms/.backup.BackupTransportService</string>

    <!-- Disable all sound effects -->
    <integer name="def_lockscreen_sounds_enabled">0</integer>
    <bool name="def_dtmf_tones_enabled">false</bool>
    <bool name="def_sound_effects_enabled">false</bool>
    <bool name="def_charging_sounds_enabled">false</bool>

    <integer name="def_location_mode">0</integer>
    <bool name="assisted_gps_enabled">false</bool>
    <bool name="def_netstats_enabled">false</bool>
    <bool name="def_haptic_feedback">false</bool>
    <bool name="def_install_non_market_apps">true</bool>
    <bool name="def_notification_pulse">false</bool>
    <integer name="def_power_sounds_enabled">0</integer>
    <bool name="def_charging_vibration_enabled">false</bool>

</resources>
